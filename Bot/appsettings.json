{
  "AllowedHosts": "*",
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "LiteDbOptions": {
    "ConnectionString": "Filename=bot.db;Connection=shared"
  },
  "AutomationWorkerOptions": {
    "Token": "{DiscordBotToken}",
    "AutomationChannelId": 0
  },
  "OmdbServiceOptions": {
    "ApiKey": "{OmdbApiKey}"
  },
  "RequestbinServiceOptions": {
    "Url": "{RequestbinUrl}"
  }
}